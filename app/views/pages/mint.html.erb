<div id="NFTMIntingForm">
  <div class="col-md-6 offset-md-3 mt-5">
    <%= simple_form_for :form , :url=> pages_path, method: :POST, id: 'formId' do |f|%>
      <%= f.input :name, :as=>:string, :label=>"NFT Name" %>
      <br>
      <%= f.input :Imgae, :as=>:file, :label=>"NFT Image"%>
      <br>
      <%= f.input :Description , :as=>:text, :label=>"Description"%>
      <br>
      <%= f.submit 'Submit'%>
    <%end%>
  </div>
</div>
